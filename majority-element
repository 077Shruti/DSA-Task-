class Solution {
    public int majorityElement(int[] nums) {
        HashMap<Integer,Integer> map=new HashMap<>();
        for(int i:nums){
            map.put(i,map.getOrDefault(i,0)+1);
        }
        int max=0;
        int ans=0;
        for(int i:map.keySet()){
            int a=map.get(i);
            if(a>max){
                max=a;
                ans=i;
            }
        }
        return ans;
        
    }
}
