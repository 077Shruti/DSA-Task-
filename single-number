class Solution {
    public int singleNumber(int[] nums) {
        HashMap<Integer,Integer> map=new HashMap<>();
        for(int i:nums){
            map.put(i,map.getOrDefault(i,0)+1);
        }
        int a=0;
        for(int i:map.keySet()){
            int b=map.get(i);
            if(b==1){
                a=i;
                break;
            }
        }
        return a;
        
    }
}
